# Copy this file to .env and fill in values

# Your Google Gemini API key (leave empty to use mock provider)
GEMINI_API_KEY=

# API key required by this service (sent as X-API-Key header)
API_KEY=honeypot-2026-02-03

# LLM Provider: "gemini" (requires GEMINI_API_KEY) or "mock" (free, heuristic-based)
LLM_PROVIDER=mock

# Optional LLM settings
# LLM_MODEL_NAME=gemini-2.5-flash-lite
# LLM_COOLDOWN_SECONDS=2
# DEFAULT_CONFIDENCE_ON_FAILURE=0.5

# Callback settings (for GUVI evaluation)
CALLBACK_ENABLED=true
CALLBACK_URL=https://hackathon.guvi.in/api/updateHoneyPotFinalResult
CALLBACK_MIN_TURNS=4
